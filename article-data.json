---
layout: null
---
{ "posts": [{% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "excerpt": "{{ post.excerpt | strip_html | strip_newlines }}",
      "publishDate": "{{ post.date }}",
      "url": "{{ site.url }}{{ post.url }}",
      "tags": "{{ post.tags | join: ' ' }}"
    } {% if post != site.posts.last %}, {% endif %} {% endfor %} ]}